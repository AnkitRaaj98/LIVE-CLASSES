   "source": [
    "def fare(x):\n",
    "    if x <=100:\n",
    "        return 'A'\n",
    "    elif x > 101 and x<200:\n",
    "        return 'B'\n",
    "    else:\n",
    "        return 'c'"
   ]
  },
  {
